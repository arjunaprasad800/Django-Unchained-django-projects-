def start_line(racestarted,racenotstarted,delay):
    while delay==5:
        return racestarted
    else:
        return racesnotstarted



    def Personality_race(name,car,car_right,car_left,car_drift, car_jump,car_throttle,car_nos,car_gearchange):
        car = name.access()
        name.access = car_right(1)
        name.access = car_left(2)
        name.access = car_drift(3)
        name.access = car_jump(4)
        name.access = car_throttle(5)
        name.access = car_nos(6)
        name.access = car_gearchange(7)
#### number of access and actions can be included###

        def points_increased(car_jump,car_throttle,car_nos,car_gearchange):
            points_increased = 2
        if car_jump ==4:
            print("acquired_points'2'",points_increased)
        elif car_throttle==5:
            print("acquired points '2'",points_increased)
        elif car_nos ==6:
            print("acquired points '2'",points_increased)
        elif car_gearchange(7):
            print ("acquired points '2'", points_increased)
        def challenges():
            drift = 3
        if drift==(3,3):
            print("challenge_completed")
        else:
            print("challenge_not_completed")
    
            jump = 4
        if jump==(3,4):
            print("challenge_completed")
        else:
            print("challenge_not_completed") 
        
            throttle = 5
        if throttle== (3,5):
            print("challenge_completed")
        else:
            print("challenge_not_completed")

            nos = 6
        if nos== (3,6):
            print("challenge_completed")
        else:
            print("challenge_not_completed")  

            gearchange = 7
        if gearchange== (3,7):
            print("challenge_completed")
        else:
            print("challenge_not_completed")

