import os
import math
import sys

def Personality(Arjuna,Razzak, Sabby, Hani, Manik):
    
    Arjuna = os.access(1)
    def body_arjuna(ear, nose,torso, legs, jeans,shirt):
        hair = os.access('$blue-500')
        face = os.access('$pale-...')
        eye = os.access('round','$black-...')
        mouth= os.access('centred','$pale-...')
        teeth = os.access('fixed','$white-...')
        ear = os.access('aligned','$pale-...')
        nose = os.access('fixed,''$pale-...')
        torso= os.access('fixed','$pale-...')
        jeans = os.access('black')
        shirt = os.access('white')
     
    if os.access(1)==Arjuna:
        return Arjuna
    else:
        return False
